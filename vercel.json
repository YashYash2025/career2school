{
  "name": "school2career-riasec-assessment",
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "env": {
    "NEXT_PUBLIC_SUPABASE_URL": "@supabase_url",
    "NEXT_PUBLIC_SUPABASE_ANON_KEY": "@supabase_anon_key"
  },
  "regions": ["iad1", "sfo1"],
  "functions": {
    "app/api/**/*.js": {
      "maxDuration": 30
    }
  }
}